{
    "name": "daemon-byte/ebay-sdk-php",
    "description": "An eBay SDK for PHP. Use the eBay API in your PHP projects.",
    "keywords": ["ebay", "api", "sdk"],
    "homepage": "https://github.com/daemon-byte/ebay-sdk-php",
    "type": "library",
    "license": "(Apache-2.0 and MIT)",
    "authors": [
        {
            "name": "GeekSeller",
            "email": "support@geekseller.com",
            "homepage": "https://www.geekseller.com"
        },
        {
            "name": "Daemon Byte",
            "homepage": "https://github.com/daemon-byte/ebay-sdk-php"
        }
    ],
    "support": {
        "issues": "https://github.com/daemon-byte/ebay-sdk-php/issues"
    },
    "require": {
        "php": ">=8",
        "guzzlehttp/guzzle": "~7.2",
        "guzzlehttp/psr7": "~2.4",
        "guzzlehttp/promises": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "DTS\\eBaySDK\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "DTS\\eBaySDK\\Test\\": "test/"
        },
        "classmap": ["build/"]
    }
}
